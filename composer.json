{
    "name": "johnnyfreeman/laravel-model-config",
    "description": "config() for your laravel models",
    "keywords": ["laravel", "model", "config"],
    "license": "MIT",
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "JohnnyFreeman\\LaravelModelConfig\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "illuminate/config": "^9.0"
    }
}
